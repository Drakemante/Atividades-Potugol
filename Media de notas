programa {
  funcao inicio() {
    real nota1
    real nota2
    real nota3
    real nota4
    real result


    escreva ("Qual a Nota 1\n")
    leia (nota1)
    escreva("Qual a Nota 2\n")
    leia (nota2)
    escreva ("Qual a Nota3\n")
    leia (nota3)
    escreva ("Qual a Nota 4\n")
    leia (nota4)

    result = (nota1 + nota2 + nota3+ nota4) / 4
escreva("Resultado\n", result)
  }
}
